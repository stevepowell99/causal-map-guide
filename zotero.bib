@book{bratovicFocusGroupResearch,
  title = {Focus Group Research with Elementary School Principals in {{Sarajevo Canton}}},
  author = {Bratovi{\'c}, Esad},
  publisher = {{proMENTE}},
  abstract = {Principals were asked about their views on parental participation. They see themselves as under pressure from parents because their childrens' grades depend on the judgement of the teaching staff which distorts grades and dominates the nature of parents' involvement in school life. Having a school psychologist and/or educational scientist ("pedagog") is seen as a major resource to improving school climate and potentially relationships with parents and students. They point out that the School Council has substantial power but is not accountable; whereas the Principal is accountable. They see parental involvement as massively important in school life but wish that more parents would take a stronger role, as partners rather than as opponents or advocates for their own children. School principals have great difficulty relating to rural families, in particular Roma, who have moved into the city and suburban areas. They connect this to what they see as a general decay in value systems and express concern that the status of principals and teachers is decreasing.},
  copyright = {All rights reserved}
}

