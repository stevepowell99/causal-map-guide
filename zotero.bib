@book{bratovicFocusGroupReport,
  title = {Focus {{Group Report}}: Parental Views on Their Inclusion in Education},
  author = {Bratovi{\'c}, Esad and {\v C}elebi{\v c}i{\'c}, Ivona},
  abstract = {Report on focus groups with parents on their participation in school life. Schools where the principal and management are open to parent participation, i.e. schools that have opened their doors to parents enjoy the highest rate of parent participation in school life. In such schools, parents feel welcome and mainly go to the schools to inquire about their children. These schools tend to support most parents' initiatives and consult them on many important issues. Parents see the principal as the most important link for parent participation. Roma are completely excluded from school life. Only one out of 16 Roma parents from the two focus groups had ever heard of a parents council. Their greatest problem remains the unjust attitude towards their children and poor relations with parents of non-Roma children. Parents of children with special needs are comparatively more involved in school life due to the lack of professional staff at schools. Parents rarely mention positive examples of participation.},
  keywords = {-education,-inclusion,-Roma,!{ESP}2}
}

@book{bratovicFocusGroupResearch,
  title = {Focus Group Research with Elementary School Principals in {{Sarajevo Canton}}},
  author = {Bratovi{\'c}, Esad},
  publisher = {{proMENTE}},
  abstract = {Principals were asked about their views on parental participation. They see themselves as under pressure from parents because their childrens' grades depend on the judgement of the teaching staff which distorts grades and dominates the nature of parents' involvement in school life. Having a school psychologist and/or educational scientist ("pedagog") is seen as a major resource to improving school climate and potentially relationships with parents and students. They point out that the School Council has substantial power but is not accountable; whereas the Principal is accountable. They see parental involvement as massively important in school life but wish that more parents would take a stronger role, as partners rather than as opponents or advocates for their own children. School principals have great difficulty relating to rural families, in particular Roma, who have moved into the city and suburban areas. They connect this to what they see as a general decay in value systems and express concern that the status of principals and teachers is decreasing.},
