@article{mooreProcessEvaluationComplex2015,
  title = {Process Evaluation of Complex Interventions: {{Medical Research Council}} Guidance},
  author = {Moore, G. F. and Audrey, S. and Barker, M. and Bond, L. and Bonell, C. and Hardeman, W. and Moore, L. and O'Cathain, A. and Tinati, T. and Wight, D. and Baird, J.},
