  author = {Nyberg, Jocke and Kadi{\'c}, Alma and {\v C}elebi{\v c}i{\'c}, Ivona},
  abstract = {The evaluation covers nine partnership projects between the member organisations of the Olof Palme International Center (hereinafter ``The Palme Center'') and their partners in Bosnia and Herzegovina. The evaluation uses two categories of criteria to analyze the projects: the partner organisation's awareness and use of eight basic criteria to be fulfilled prior to a development cooperation agreement (taken from the Palme Center's handbook) and the so called assessment criteria used by Sida. The latter include the project's relevance and feasibility, how the Swedish organisations handle risk and risk management; the quality of the partnership and the sustainability of the activities initiated by the project. Within each criteria a number of specific questions \textendash{} mentioned in the terms of reference \textendash{} were analyzed during the evaluation.}
}

@article{nyenswahEvidenceDecliningNumbers2014,
  title = {Evidence for Declining Numbers of Ebola Cases - Montserrado County, Liberia, June-October 2014},
  author = {Nyenswah, TG Tolbert G and Westercamp, Matthew and Kamali, AA Amanda Ashraf AA and Qin, Jin and {Zielinski-Gutierrez}, Emily and Amegashie, Fred and Fallah, Mosaka and Gergonne, Bernadette and {Nugba-Ballah}, Roselyn and Singh, Gurudev and {Aberle-Grasse}, John M and Havers, Fiona and Montgomery, Joel M and Bawo, Luke and Wang, Susan A and Rosenberg, Ronald},
  year = {2014},
  volume = {63},
  pages = {1072--1076},
  issn = {1545-861X},
  abstract = {The epidemic of Ebola virus disease (Ebola) in West Africa that began in March 2014 has caused approximately 13,200 suspected, probable, and confirmed cases, including approximately 6,500 in Liberia. About 50\% of Liberia's reported cases have been in Montserrado County (population 1.5 million), the most populous county, which contains the capital city, Monrovia. To examine the course of the Ebola epidemic in Montserrado County, data on Ebola treatment unit (ETU) admissions, laboratory testing of patient blood samples, and collection of dead bodies were analyzed. Each of the three data sources indicated consistent declines of 53\%-73\% following a peak incidence in mid-September. The declines in ETU admissions, percentage of patients with reverse transcription-polymerase chain reaction (RT-PCR) test results positive for Ebola, and dead bodies are the first evidence of reduction in disease after implementation of multiple prevention and response measures. The possible contributions of these interventions to the decline is not yet fully understood or corroborated. A reduction in cases suggests some progress; however, eliminating Ebola transmission is the critical goal and will require greatly intensified efforts for complete, high-quality surveillance to direct and drive the rapid intervention, tracking, and response efforts that remain essential.},
  isbn = {1545-861X (Electronic)\$\textbackslash backslash\$r0149-2195 (Linking)},
  journal = {MMWR. Morbidity and mortality weekly report},
  keywords = {epi},
  number = {46},
  pmid = {25412066}
}

