  title = {A Methodology for Constructing Collective Causal Maps},
  author = {Scavarda, Annibal Jos{\'e} and {Bouzdine-Chameeva}, Tatiana and Goldstein, Susan Meyer and Hays, Julie M. and Hill, Arthur V.},
