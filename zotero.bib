  isbn = {9780395615560},
  journal = {American Journal of Evaluation},
  keywords = {developments in quantitative research,fundamental and relatively rapid,methodology have reshaped,program evaluation,quantitative methodology,researchers and evaluators,textbooks,the identification,the knowledge landscape for,with recent advances in},
  number = {3}
}

