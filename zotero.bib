@book{asiandevelopmentbankCambodiaAddressingSkills2015,
  title = {Cambodia Addressing the Skills Gap: {{Employment}} Diagnostic Study},
  author = {{Asian Development Bank}},
  year = {2015},
  issn = {01950118},
  abstract = {The skills gap is an ever-present reality for most organizations. Demographic shifts in the workforce, the globalization of human capital, educational attainment lagging the need for skills, and the impact that the global recession has had on organizations of all sizes and in all sectors mean senior executives are facing real challenges when it comes to managing the skills gap in their organizations. In fact a 2009 ASTD Skills Gap poll taken by 1,179 organizations found that 79 percent reported a skills gap in their organization. ASTD defines a skills gap as a significant gap between an organization's current capabilities and the skills it needs to achieve its goals. It is the point at which an organization can no longer grow or remain competitive because it cannot fill critical jobs with employees who have the right knowledge, skills, and abilities. In reality, organizations will always experience a skills gap if they are staying ahead of shifting conditions in their environment and changing expectations from their constituents, shareholders, or customers. The key to achieving success under such circumstances is to harness skilled talent to strategy and goals\textemdash a task that has become more and more challenging in an increasingly global, virtual, and changing world. This Infoline provides background for understanding the skills gap and an action plan for taking charge of it. The plan delineates six steps and their processes that can help leaders, managers, and learning professionals assess whether a skills gap exists and, if so, how to successfully manage it within their organizations. Companies, that successfully implemented these steps, share case studies so that their best practices can help you think creatively about solutions to tackle a skills gap in your organization.},
  file = {C\:\\Users\\steve\\Zotero\\storage\\DCZI5YLE\\Asian Development Bank - 2015 - Cambodia addressing the skills gap Employment diagnostic study.pdf},
  isbn = {978-1-56286-731-7}
}

