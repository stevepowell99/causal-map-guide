@article{murrayr.barrickBigFivePersonality1991,
  title = {The {{Big Five}} Personality Dimensions and Job Performance: {{A}} Meta-Analysis},
  author = {{Murray R. Barrick} and {Michael K. Mount}},
  year = {1991},
  pages = {1--26},
  abstract = {Investigated the relation of 5 personality dimensions (extraversion, emotional stability, agreeableness, conscientiousness, and Openness to experience) to 3 job performance criteria (job proficiency, training proficiency, and personnel data) for 5 occupational groups (professionals, police, managers, sales, and skilled/semi-skilled). A review of 117 studies (published 1952-1988) yielded 162 samples totaling 23,994 Ss. Conscientiousness showed consistent relations with all job performance criteria for all occupational groups. Extraversion was a valid predictor for 2 occupations involving social interaction, (managers and sales). Also, openness to experience and extraversion were valid predictors of the training proficiency criterion across occupations. Overall, results illustrate the benefits of using the 5-factor model of personality to accumulate empirical findings. Findings have implications for research and practice in personnel psychology. (PsycINFO Database Record (c) 2007 APA, all rights reserved)}
}

@article{nadkarniBayesianNetworkApproach2001,
  title = {Bayesian Network Approach to Making Inferences in Causal Maps},
  author = {Nadkarni, Sucheta and Shenoy, Prakash P.},
  year = {2001},
  journal = {European Journal of Operational Research},
  volume = {128},
  number = {3},
  pages = {479--498},
  issn = {03772217},
  doi = {10.1016/S0377-2217(99)00368-9},
  abstract = {The main goal of this paper is to describe a new graphical structure called `Bayesian causal maps' to represent and analyze domain knowledge of experts. A Bayesian causal map is a causal map, i.e., a network-based representation of an expert's cognition. It is also a Bayesian network, i.e., a graphical representation of an expert's knowledge based on probability theory. Bayesian causal maps enhance the capabilities of causal maps in many ways. We describe how the textual analysis procedure for constructing causal maps can be modified to construct Bayesian causal maps, and we illustrate it using a causal map of a marketing expert in the context of a product development decision.},
  annotation = {230 citations (Semantic Scholar/DOI) [2021-02-20]},
  file = {C\:\\Users\\steve\\Zotero\\storage\\T47E7II2\\Nadkarni, Shenoy - 2001 - Bayesian network approach to making inferences in causal maps.pdf}
}

