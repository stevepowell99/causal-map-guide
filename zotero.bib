@article{tolinSexDifferencesTrauma2008,
  title = {Sex Differences in Trauma and Posttraumatic Stress Disorder: {{A}} Quantitative Review of 25 Years of Research.},
  author = {Tolin, David F. and Foa, Edna B.},
  year = {2008},
  pages = {37--85},
  abstract = {This reprinted article originally appeared in Psychological Bulletin, 2006, Vol 132(6), 959-992. (The following abstract of the original article appeared in record 2006-20202-007.) Meta-analyses of studies yielding sex-specific risk of potentially traumatic events (PTEs) and posttraumatic stress disorder (PTSD) indicated that female participants were more likely than male participants to meet criteria for PTSD, although they were less likely to experience PTEs. Female participants were more likely than male participants to experience sexual assault and child sexual abuse, but less likely to experience accidents, nonsexual assaults, witnessing death or injury, disaster or fire, and combat or war. Among victims of specific PTEs (excluding sexual assault or abuse), female participants exhibited greater PTSD. Thus, sex differences in risk of exposure to particular types of PTE can only partially account for the differential PTSD risk in male and female participants. (PsycINFO Database Record (c) 2008 APA, all rights reserved)}
}

