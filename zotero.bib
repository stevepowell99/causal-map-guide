  author = {{Kuterovac-Jagodi{\'c}}, Gordana},
  year = {2003},
  volume = {59},
  pages = {9--25},
  issn = {00219762},
  doi = {10.1002/jclp.10114},
  abstract = {This study examined symptoms of posttraumatic stress in 252 school-aged children from Osijek, Croatia, which was subjected to massive military attacks from Yugoslavian forces. The children's symptoms were assessed in 1994 while the war was still going on and 30 months later when the war was over. In addition to changes in posttraumatic stress disorder symptoms over time, the study examined the predictive power of (a) different types and number of war traumata, (b) loss of social community, (c) the children's demographic characteristics (age and gender), (d) types of coping strategies and locus of control, and (e) the perceived availability of different kinds of social support. Although symptoms of posttraumatic stress declined over time, 10\% of the children reported a severe level of symptomatology 30 months after the war. The results supported the hypothesized predictive power of all investigated factors for predicting short- and long-term posttraumatic stress reactions.},
  isbn = {0021-9762 (Print)\$\textbackslash backslash\$n0021-9762 (Linking)},
  journal = {Journal of Clinical Psychology},
  keywords = {Children,Croatia,Longitudinal study,Posttraumatic stress reactions,War},
  number = {1},
  pmid = {12508328}
}

