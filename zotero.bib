@article{zadehOutlineNewApproach1973,
  title = {Outline of a {{New Approach}} to the {{Analysis}} of {{Complex Systems}} and {{Decision Processes}}},
  author = {Zadeh, Lotfi},
  year = {1973},
  journal = {Systems, Man and Cybernetics, IEEE Transactions on},
  volume = {SMC-3},
