  shorttitle = {Evaluacija {{Programa}} Obuke Za Odabir Zanimanja i Ra},
  author = {Husremovi{\'c}, D{\v z}enana and Lepi{\'c}, Sidik},
  abstract = {In the school year of 2010/2011, \{GOPA\} \textendash\{YEP\} (Youth Employment Program, Component 1) and \{proMENTE\} Social Research prepared and implemented a training project for secondary school students to select their job and develop their career. \{YEP\} Project Component 1 was aimed at developing capacities of secondary school students for competent and educated decision-making regarding continuation of their education and job searching. The capacity-building process was conducted through workshops during the whole school year 2010/2011 in 6 vocational secondary schools in Bosnia and Herzegovina. Six schools took part in the project: Mixed Secondary School Gra\v{c}anica, Secondary School Prnjavor, Civil Construction \textendash{} Geodetic School Tuzla (four-year course), Secondary Technical School Br\v{c}ko, Secondary School Center ``Milorad Vla\v{c}i\'c'' Vlasenica, and Secondary School Rogatica. This document presents the results of the program.},
  copyright = {All rights reserved}
}

