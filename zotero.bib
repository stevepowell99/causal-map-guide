@book{powellESPSchoolPrincipals,
  type = {Statistical Report},
  title = {\{\vphantom\}{{ESP}}\vphantom\{\} School Principals Research: \{\vphantom\}{{BiH}}\vphantom\{\}},
  author = {Powell, Steve},
  publisher = {{proMENTE}},
  abstract = {The results for Bosnia and Herzegovina (B\$\textbackslash backslash\$\&H) of a survey on 237 elementary school principals' attitudes towards and views on parental participation in schools. Forms of participation in \{BiH\} schools do not extend to parental influence over education or the running of the school but does include organisation of events, renovating buildings etc. Many schools have quite frequent, but limited, one-way contact with parents. Attendance at meetings is relatively good overall. However there are some groups of parents who attend much less frequently. Parents participate more in bigger schools who consider themselves to be relatively well resourced and in which the principal is convinced of the value of participation. When principals are convinced of the value of participation, they make more effort to encourage it and it is indeed more successful.},
  copyright = {All rights reserved}
}

