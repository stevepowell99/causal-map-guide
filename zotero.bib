@article{smallUsingInterviewsUnderstand2021,
  title = {Using {{Interviews}} to {{Understand Why}}: {{Challenges}} and {{Strategies}} in the {{Study}} of {{Motivated Action}}},
  author = {Small, Mario L and Cook, Jenna M},
  year = {2021},
